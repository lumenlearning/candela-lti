{
  "name": "lumenlearning/candela-lti",
  "license": "GPL-2.0+",
  "version": "1.0.0",
  "type": "wordpress-plugin",
  "description":
    "A plugin that implements the LTI/Wordpress integration.",
  "homepage": "https://github.com/lumenlearning/candela-lti",
  "authors": [
    {
      "name": "Lumen Learning",
      "email": "dev@lumenlearning.com",
      "homepage": "https://lumenlearning.com"
    }
  ],
  "support": {
    "email": "dev@lumenlearning.com",
    "issues": "https://github.com/lumenlearning/candela-lti/issues/",
    "source": "https://github.com/lumenlearning/candela-lti/"
  },
  "config": {
    "platform": {
      "php": "7.1"
    }
  },
  "require": {
    "php": ">=7.1",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.2",
    "squizlabs/php_codesniffer": "^3.4.0",
    "wp-coding-standards/wpcs": "dev-master"
  },
  "scripts": {
    "test": [
      "@standards"
    ],
    "standards": [
      "vendor/bin/phpcs --standard=phpcs.ruleset.xml *.php ."
    ]
  }
}
